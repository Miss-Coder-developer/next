{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown",
		"better-docs/component",
		"better-docs/category"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"better-docs": {
			"name": "React components"
		}
	},
	"opts": {
		"template": "node_modules/better-docs",
		"recurse": true,
		"destination": "./docs/docPage",
		"readme": "README.md"
	}
}
